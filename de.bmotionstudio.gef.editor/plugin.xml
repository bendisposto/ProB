            class="de.bmotionstudio.gef.editor.StaticListenerRegistry">
      </listener>
   </extension>
   <extension
         point="de.prob.core.lifecycle">
      <listener
            class="de.bmotionstudio.gef.editor.StaticListenerRegistry">
      </listener>
   </extension>
   <extension
